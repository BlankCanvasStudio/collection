[Unit]
Description=sticky elephant run script
Wants=network-online.target
After=network-online.target

[Service]
Restart=always
RestartSec=1s

WorkingDirectory=/home/{{ ansible_user_id }}/sticky_elephant
ExecStart=/bin/bash -c './bin/sticky_elephant'

TimeoutStartSec=300
TimeoutStopSec=60

[Install]
WantedBy=multi-user.target
