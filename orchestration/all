#!/bin/bash

./orchestration/run    sunsh 20 ../official/data/legitimate/dnsmitm       dnsmitm
./orchestration/run -f sunsh 20 ../official/data/legitimate/gpt           gpt
./orchestration/run -f sunsh 20 ../official/data/legitimate/imgnet        imgnet
./orchestration/run    sunsh 20 ../official/data/legitimate/packetsniffer packetsniffer
./orchestration/run    sunsh 20 ../official/data/legitimate/synflood      synflood
./orchestration/run    sunsh 20 ../official/data/malicious/portscanner    portscanner
./orchestration/run -f sunsh 20 ../official/data/malicious/ransomware     ransomware
./orchestration/run    sunsh 20 ../official/data/malicious/spread         spread
./orchestration/run    sunsh 20 ../official/data/malicious/upload         upload

cd ../official && git add * && git commit -m "re-running all the data" && git push

